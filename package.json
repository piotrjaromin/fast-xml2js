{
  "name": "@pjaromin/fast-xml2js",
  "version": "1.3.0",
  "description": "Fork of cmrigney/fast-xml2js. In-place replacement for xml2js parseString. This is about 20x-30x faster and makes use of the rapidxml C++ library.",
  "main": "index.js",
  "scripts": {
    "preinstall": "node-gyp configure && node-gyp build",
    "compile": "node-gyp rebuild",
    "test": "mocha --recursive test",
    "bench": "node benchmark/benchmark.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piotrjaromin/fast-xml2js.git"
  },
  "keywords": [
    "xml2js",
    "C++"
  ],
  "author": "Cody Rigney",
  "license": "MIT",
  "gypfile": true,
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "benchmark": "^2.1.4",
    "microtime": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "nan": "^2.14.0",
    "node-gyp": "^6.0.1"
  }
}
